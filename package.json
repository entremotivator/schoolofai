{
  "name": "psuvr",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "frontend-dev": "vite --host",
    "frontend-build": "vite build",
    "backend-build": "node server.js",
    "backend-dev": "nodemon server.js",
    "dev": "concurrently --kill-others \"npm run frontend-dev\" \"npm run backend-dev\"",
    "start": "vercel dev",
    "vercel-build": "vite build",
    "lint": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,scss,md}\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "gsap": "^3.12.2",
    "include-media": "^2.0.0",
    "nodemon": "^2.0.22",
    "sass": "^1.63.6",
    "vite": "^4.3.9",
    "eslint": "^8.56.0",
    "prettier": "^3.1.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "nipplejs": "^0.10.1",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.1",
    "three": "^0.154.0"
  }
}
